{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": [2, "always"],
        "quotes": [2, "single"],
        "no-trailing-spaces": [2, { "skipBlankLines": true }],
        "indent": ["error", 2],
        "no-unused-vars": ["warn", { "vars": "all" }]
    }
}
